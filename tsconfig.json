{
    "compilerOptions": {
      "target": "ES2022",
      "strict": true,
      "noUnusedLocals": false,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
  
      "noImplicitAny": true,
      "esModuleInterop": true,
      "moduleResolution": "node"
    }
  }